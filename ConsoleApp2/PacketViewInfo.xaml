        <TextBox x:Name="PacketText" HorizontalAlignment="Left" Height="338" Margin="67,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="625" VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" IsReadOnly="True" Padding="20" FontSize="14"/>
    </Grid>
